{
    "name": "PersonalWebsite",
    "version": "1.0.0",
    "description": "Personal Website written in typescript using node and restify.",
    "main": "public/app/app.js",
    "scripts": {
        "pretest": "npm run compile",
        "test": "node_modules/.bin/_mocha public/test/unit/**/*.spec.js",
